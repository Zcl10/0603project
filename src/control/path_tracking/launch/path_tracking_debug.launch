<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="launch_prefix" default="xterm -e gdb --args"/>
  <arg name="config" default="$(find path_tracking)/config/config.yaml" />

  <node pkg="path_tracking" type="mpc_controller" name="mpc_controller" output="screen" launch-prefix="$(arg launch_prefix)">
    <rosparam command="load" file="$(arg config)"/>
  </node>
</launch>
